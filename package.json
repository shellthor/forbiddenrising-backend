    "@types/express": "^4.17.8",
    "@types/jest": "26.0.15",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.162",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.14.2",
    "@types/passport-discord": "^0.1.3",
    "@types/passport-oauth2": "^1.4.9",
