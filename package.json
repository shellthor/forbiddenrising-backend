{
  "name": "forbiddenrising-api",
  "version": "0.0.1",
  "description": "API backend for the forbidden rising gaming community",
  "author": "Jeff Detmer <jeff.detmer@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn prebuild && nest build",
    "format": "prettier \"**/*.{ts,js,json,md,yml}\" --ignore-path .gitignore --write",
    "start": "nest start",
    "start:dev-cli": "nest start --watch",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node -r dotenv/config dist/main.js\"",
    "start:debug-cli": "nest start --debug --watch",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect -r dotenv/config dist/main.js\"",
    "start:prod": "node -r dotenv/config dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --color --runInBand --detectOpenHandles --forceExit",
    "orm:create": "ts-node -r dotenv/config create-schema",
    "commit": "cz"
  },
  "dependencies": {
    "@compodoc/compodoc": "^1.1.11",
