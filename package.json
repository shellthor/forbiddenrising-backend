{
  "name": "guild-backend",
  "version": "0.0.1",
  "description": "API backend for my gaming community",
  "author": "Jeff Detmer <jeff.detmer@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier \"**/*.{ts,js,json,md,yml}\" --ignore-path .gitignore --write",
    "start": "nest start",
    "start:dev-cli": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/src/main.js\"",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --ci --color",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --color --runInBand --detectOpenHandles --forceExit",
    "orm:create": "ts-node create-schema",
    "orm:migration:create": "npx @mikro-orm/migrations migration:create"
  },
  "dependencies": {
    "@discordjs/opus": "^0.3.2",
    "@golevelup/nestjs-discovery": "^2.3.1",
    "@mikro-orm/cli": "^4.0.0-alpha.0",
    "@mikro-orm/core": "^4.0.0-alpha.0",
    "@mikro-orm/knex": "^4.0.0-alpha.0",
    "@mikro-orm/migrations": "^4.0.0-alpha.0",
    "@mikro-orm/nestjs": "^4.0.0-alpha.3",
    "@mikro-orm/postgresql": "^4.0.0-alpha.0",
    "@mikro-orm/reflection": "^4.0.0-alpha.0",
    "@nestjs/bull": "^0.1.2",
    "@nestjs/common": "^7.4.3",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.4.3",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.4.3",
    "@nestjs/platform-socket.io": "^7.4.3",
    "@nestjs/swagger": "^4.6.0",
    "@nestjs/websockets": "^7.4.3",
    "@ntegral/nestjs-sentry": "^1.0.4",
    "accesscontrol": "^2.2.1",
    "axios": "^0.20.0",
    "bull": "^3.18.0",
    "cache-manager": "^3.3.0",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "date-fns": "^2.16.0",
    "discord.js": "^12.3.1",
    "dotenv": "^8.2.0",
    "ffmpeg-static": "^4.2.7",
    "helmet": "^4.1.0",
    "joi": "^17.2.1",
    "localtunnel": "^2.0.0",
    "lodash": "^4.17.20",
    "multer": "^1.4.2",
    "nest-schedule": "^0.6.4",
    "nestjs-admin": "^0.4.0",
    "p-queue": "^6.6.1",
    "passport": "^0.4.1",
    "passport-bnet": "^2.0.0",
    "passport-discord": "^0.1.4",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.3.3",
    "ramda": "^0.27.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.2",
    "serverless-http": "^2.5.0",
    "slugify": "^1.4.5",
    "swagger-ui-express": "^4.1.4",
    "ytdl-core": "^3.2.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.1",
    "@nestjs/testing": "^7.4.3",
    "@types/bull": "^3.14.2",
    "@types/express": "^4.17.7",
    "@types/jest": "26.0.10",
    "@types/joi": "^14.3.4",
    "@types/lodash": "^4.14.161",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.6.2",
    "@types/passport-discord": "^0.1.3",
    "@types/passport-oauth2": "^1.4.9",
    "@types/ramda": "^0.27.14",
    "@types/socket.io": "^2.1.11",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "commitizen": "4.2.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "26.4.2",
    "prettier": "^2.1.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.3.0",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "tsc-watch": "^4.2.9",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2"
  },
  "resolutions": {
    "chokidar": "^3.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "collectCoverage": true
  },
  "mikro-orm": {
    "useTsNode": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
